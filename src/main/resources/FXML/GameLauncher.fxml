<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="View.Wave"
      prefHeight="600.0" prefWidth="900.0"
      stylesheets="@../CSS/styles.css">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Atomic Bomber"/>
        <ImageView fx:id="groundImageView" fitHeight="600.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../images/ground1.png"/>
        </ImageView>
        <ImageView fx:id="avatarImageView" fitHeight="40.0" fitWidth="40.0" translateX="14.0" translateY="500.0" pickOnBounds="true" preserveRatio="true" />
    </children>

</Pane>
